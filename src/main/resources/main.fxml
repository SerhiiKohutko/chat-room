   <children>
      <VBox layoutX="14.0" layoutY="14.0" prefHeight="275.0" prefWidth="571.0" style="-fx-background-color: gray;">
         <children>
            <TextArea fx:id="textArea" editable="false" prefHeight="282.0" prefWidth="571.0" wrapText="true">
               <font>
                  <Font name="Comic Sans MS" size="12.0" />
               </font></TextArea>
         </children></VBox>
      <Button fx:id="sendButton" layoutX="515.0" layoutY="321.0" mnemonicParsing="false" onAction="#sendMessage" text="Send" />
      <TextField fx:id="textField" layoutX="14.0" layoutY="321.0" prefHeight="30.0" prefWidth="495.0" />
   </children>
</AnchorPane>
